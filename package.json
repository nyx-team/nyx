  "dependencies": {
    "@discordjs/builders": "^1.0.0",
    "@discordjs/rest": "^1.0.0",
    "axios": "^0.27.2",
    "bufferutil": "^4.0.6",
    "chalk": "^5.0.1",
    "discord.js": "^14.0.1",
    "erlpack": "github:discord/erlpack",
    "moment": "^2.29.4",
    "mongoose": "^6.4.4",
    "ts-node": "^10.9.1",
    "utf-8-validate": "^5.0.9",
    "zlib-sync": "^0.1.7"
