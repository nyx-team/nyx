{
  "name": "nyx",
  "version": "1.0.0",
  "description": "Nyx is a multi-purpose discord bot",
  "main": "index.js",
  "engines": {
    "node": "v17.x"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon index.ts --dev",
