{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": "off",
        "curly": "off",
        "global-require": "off",
        "import/no-dynamic-require": "off",
        "consistent-return": "off"
    }
}
